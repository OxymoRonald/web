<html>
    <head>
        <meta charset="UTF-8">
        <title>YellowJack POS</title>
        <link type="text/css" rel="stylesheet" href="style.css">
    </head>
    <body>
        <div id="header">Page title</div>
        <div id="main">
            <div id="left_block">
                <table class="itemlist">
                    <thead>
                        <tr>
                            <th colspan="4">Items</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="item_image"><img src="images/burger.png" alt="Burger"></td>
                            <td class="item_body">
                                <div class="item_title">Item Title</div>
                                <div class="item_description">Item description</div>
                            </td>
                            <td><div class="item_counter">2</div></td>
                            <td class="item_add">
                                <div class="item_buttons">
                                    <img src="green_small.png" alt="Add">
                                    <img src="red_small.png" alt="Subtract">
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td class="item_image"><img src="images/fries.png" alt="Fries"></td>
                            <td class="item_body">Description2</td>
                            <td><div class="item_counter">2</div></td>
                            <td class="item_add">+<br/>-</td>
                        </tr>
                        <tr>
                            <td class="item_image"><img src="images/rootbeer.png" alt="Rootbeer"></td>
                            <td class="item_body">Description3</td>
                            <td><div class="item_counter">2</div></td>
                            <td class="item_add">+<br/>-</td>
                        </tr>
                    </tbody>
                </table>
                <table class="itemlist">
                    <thead>
                        <tr>
                            <th colspan="3">Combo's</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="item_image">Image</td>
                            <td class="item_body">
                                <div class="item_title">Item Title</div>
                                <div class="item_description">Item description</div>
                            </td>
                            <td class="item_add">+</td>
                        </tr>
                        <tr>
                            <td class="item_image">Image2</td>
                            <td class="item_body">Description2</td>
                            <td class="item_add">+</td>
                        </tr>
                        <tr>
                            <td class="item_image">Image3</td>
                            <td class="item_body">Description3</td>
                            <td class="item_add">+</td>
                        </tr>
                        <tr>
                            <td class="item_image">Image3</td>
                            <td class="item_body">Description3</td>
                            <td class="item_add">+</td>
                        </tr>
                        <tr>
                            <td class="item_image">Image3</td>
                            <td class="item_body">Description3</td>
                            <td class="item_add">+</td>
                        </tr>
                    </tbody>
                </table>
                <table class="itemlist">
                    <thead>
                        <tr>
                            <th colspan="3">Misc</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="item_image">Image</td>
                            <td class="item_body">
                                <div class="item_title">Item Title</div>
                                <div class="item_description">Item description</div>
                            </td>
                            <td class="item_add">+</td>
                        </tr>
                        <tr>
                            <td class="item_image">Image2</td>
                            <td class="item_body">Description2</td>
                            <td class="item_add">+</td>
                        </tr>
                        <tr>
                            <td class="item_image">Image3</td>
                            <td class="item_body">Description3</td>
                            <td class="item_add">+</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div id="right_block">Right content</div>
        </div>    
    </body>
</html>
